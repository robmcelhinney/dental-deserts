# Copy to .env and set values
NHS_API_SUBSCRIPTION_KEY="your-key-here"
NHS_API_SUBSCRIPTION_SECRET="optional-secret-if-issued"

NHS_SERVICE_SEARCH_URL="https://int.api.service.nhs.uk/service-search-api"
NHS_SERVICE_SEARCH_API_VERSION="3"
NHS_SERVICE_SEARCH_QUERY="dentist"
NHS_SERVICE_SEARCH_PAGE_SIZE="200"
NHS_SERVICE_SEARCH_MAX_PAGES="80"

# Optional: LSOA boundaries source (GeoJSON or .geojson.gz)
LSOA_BOUNDARIES_URL=""

# Optional override for IMD source CSV (default is IoD 2025 File 7 on GOV.UK assets)
IMD_SOURCE_URL=""
